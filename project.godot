; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[Debug]

debug_mode=true

[application]

config/name="Castle Yokai"
run/main_scene="res://src/UI/SplashScreen/SplashScreen.tscn"
config/icon="res://icon.png"

[audio]

output_latency=50
output_latency.web=100

[autoload]

Actions="*res://src/settings/Actions.gd"
Constants="*res://src/settings/Constants.gd"
LevelData="*res://src/settings/LevelData.gd"
Global="*res://src/UI/Global.tscn"
Game_AudioManager="*res://src/lib/AudioManager/AudioManager.tscn"
Settings="*res://src/settings/Settings.gd"
ArrayUtil="*res://src/utility/ArrayUtil.gd"
GameState="*res://src/settings/GameState.gd"
Time="*res://src/utility/Time.gd"
Ease="*res://src/utility/Ease.gd"
Projectiles="*res://src/objects/test-objects/Guns/Projectiles.tscn"

[display]

window/size/width=320
window/size/height=176
window/size/always_on_top=true
window/size/test_width=960
window/size/test_height=528
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/LDtk-Importer/plugin.cfg", "res://addons/project_map/plugin.cfg", "res://addons/vnen.tiled_importer/plugin.cfg" )

